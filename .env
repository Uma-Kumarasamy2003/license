# Backend Environment Variables (.env file for local development)
# Copy this file to .env and fill in your actual values

# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/licenseDB?retryWrites=true&w=majority&appName=Cluster0

# Encryption Key (32 characters recommended)
ENCRYPTION_KEY=your-secret-key-here-32-chars!!

# Environment
NODE_ENV=development

# Server Port (optional, defaults to 5000)
PORT=5000